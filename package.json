{
  "name": "lspa",
  "version": "1.0.0",
  "description": "Angular SPA frontend for the Limitless Garden API",
  "repository": "https://github.com/abarbanell/lspa",
  "scripts": {
    "start": "node server.js",
    "start:full": "npm run rebuild && npm start",
    "start:dev": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start --single-run",
    "test:w": "karma start --auto-watch",
    "clean": "rimraf node_modules && npm install",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "rebuild": "npm run clean && npm run build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "bootstrap": "3.3.7",
    "core-js": "^2.4.1",
    "express": "4.14.0",
    "install": "0.8.4",
    "jquery": "3.1.1",
    "npm": "4.1.1",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "typescript": "~2.0.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "^0.14.0"
  }
}
